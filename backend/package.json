{
  "name": "mea2n-kickstarter",
  "version": "0.0.0",
  "description": "Kickstart your development with a strong foundation",
  "main": "lib/src/main.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "build": {
    "port": 3000
  },
  "scripts": {
    "install": "typings install;",
    "build": "tsc; babel . --out-dir lib --source-maps --ignore lib,node_modules;",
    "test": "npm run build; mocha lib/tests;",
    "watch": "concurrently -p name --names TSC,BABEL --prefix-colors yellow,blue --kill-others \"tsc --project tsconfig.json --watch\" \"babel . --watch --out-dir ./lib --source-maps --ignore lib,node_modules\"",
    "start": "concurrently --raw --kill-others \"npm run watch\" \"nodemon --ignore ./src --delay 1 lib/src/main.js\""
  },
  "author": "Arie Milner",
  "license": "GPL-3.0",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.14.2",
    "connect-mongo": "^1.2.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "kleen": "^3.0.0",
    "moment": "^2.17.1",
    "mongodb": "^2.1.7",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "ramda": "^0.21.0",
    "request": "^2.69.0",
    "systemjs": "^0.19.16",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "concurrently": "^3.4.0",
    "mocha": "^3.0.2",
    "nodemon": "^1.11.0",
    "typescript": "^2.3.0-dev.20170318",
    "typings": "^1.3.1"
  }
}
